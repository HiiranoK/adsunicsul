algoritmo divisãoInteiros
    inteiro dividendo, divisor;
    início
        escreva("Informe o valor do dividendo");
        leia(numero1);
        escreva("informe o valor do divisor");
        leia(numero2);
        escreva("O resto da divisão de" + numero1 + " por " + numero2 + " é:" + numero1 % numero2)
    fim