algoritmo Hipotenusa
    real comprimentoA, comprimentoB,hipotenusa;
    início
        escreva("informe a metragem da rua do Sol");
        leia(comprimentoA);
        escreva("informe a metragem da estrada dos pássaros verdes");
        leia(comprimentoB);
        
        hipotenusa = raiz(comprimentoA * comprimentoA + comprimentoB * comprimentoB)
    fim