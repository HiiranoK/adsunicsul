algoritmo Exercicio8
    real alturaDegrau, alturaTotal;
    início
        escreva("Informe a altura do degrau (em metros)");
        leia(alturaDegrau); //0.5 metros
        escreva("Informe quantos metros deseja subir (em metros)");
        leia(alturaTotal); // 10 metros
        escreva("Para subir essa altura serão necessários:" +  alturaTotal/alturaDegrau + " degrais!")  
    fim