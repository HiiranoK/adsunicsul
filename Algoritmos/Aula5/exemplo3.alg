algoritmo Exemplo3
    real notaUm, notaDois;
    início
        escreva("Informe a primeira nota");
        leia notaUm;
        escreva("Informe a segunda nota");
        leia notaDois;
        se((notaUm + notaDois)/2 >=6){
            escreva("Você passou")
        }senao{
            escreva("Você reprovou")
        }
    fim